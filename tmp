--
-- Temporary table structure for view `Übersicht_einheiten`
--

DROP TABLE IF EXISTS `Übersicht_einheiten`;
/*!50001 DROP VIEW IF EXISTS `Übersicht_einheiten`*/;
/*!50001 CREATE TABLE `Übersicht_einheiten` (
  `einheiten_id` int(10) unsigned,
  `angebote_id` int(10) unsigned,
  `Datum` date,
  `Auftraggeber` varchar(50),
  `Inhalt` varchar(60),
  `Preis` float
) ENGINE=MyISAM */;

--
-- Final view structure for view `Übersicht_einheiten`
--

/*!50001 DROP TABLE `Übersicht_einheiten`*/;
/*!50001 DROP VIEW IF EXISTS `Übersicht_einheiten`*/;
/*!50001 CREATE ALGORITHM=UNDEFINED */
/*!50013 DEFINER=`markus`@`` SQL SECURITY DEFINER */
/*!50001 VIEW `Übersicht_einheiten` AS select distinct `Arbeitrechnungen`.`einheiten`.`einheiten_id` AS `einheiten_id`,`Arbeitrechnungen`.`einheiten`.`angebote_id` AS `angebote_id`,`Arbeitrechnungen`.`einheiten`.`Datum` AS `Datum`,`Arbeitrechnungen`.`klienten`.`Auftraggeber` AS `Auftraggeber`,`Arbeitrechnungen`.`angebote`.`Inhalt` AS `Inhalt`,`Arbeitrechnungen`.`angebote`.`Preis` AS `Preis` from ((`einheiten` join `klienten`) join `angebote`) where ((`Arbeitrechnungen`.`einheiten`.`angebote_id` = `Arbeitrechnungen`.`angebote`.`angebote_id`) and (`Arbeitrechnungen`.`einheiten`.`klienten_id` = `Arbeitrechnungen`.`klienten`.`klienten_id`) and (`Arbeitrechnungen`.`angebote`.`klienten_id` = `Arbeitrechnungen`.`klienten`.`klienten_id`)) */;

CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC

CREATE TABLE angebote(angebote_id INTEGER unsigned NOT NULL auto_increment,
  `klienten_id` int(10) unsigned default NULL,
  `Inhalt` varchar(60) NOT NULL,
  `Preis` float NOT NULL,
  `preis_pro_stunde` tinyint(1) NOT NULL default '0' COMMENT 'wenn wahr (1), muss der Preis mit Stundenanzahl multipliziert werden',
  `Beschreibung` text,
  PRIMARY KEY  (`angebote_id`),
  KEY `COUNTRIES_FK` (`klienten_id`),
  CONSTRAINT `angebote_ibfk_1` FOREIGN KEY (`klienten_id`) REFERENCES `klienten` (`klienten_id`) ON DELETE SET NULL
)

LOCK TABLES `angebote` WRITE;
/*!40000 ALTER TABLE `angebote` DISABLE KEYS */;

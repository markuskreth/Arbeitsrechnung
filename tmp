--
-- Table structure for table `einheiten`
--

DROP TABLE IF EXISTS `einheiten`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `einheiten` (
  `einheiten_id` int(10) unsigned NOT NULL auto_increment,
  `klienten_id` int(10) unsigned NOT NULL,
  `angebote_id` int(10) unsigned NOT NULL,
  `Datum` date NOT NULL,
  `Beginn` datetime default NULL COMMENT 'Start der Stunde',
  `Ende` datetime default NULL COMMENT 'Ende der Stunde',
  `Dauer` float NOT NULL default '0',
  `zusatz1` varchar(255) default NULL,
  `zusatz2` varchar(255) default NULL,
  `Preis` float NOT NULL default '0' COMMENT 'Summe für die Einheit',
  `Preisänderung` float NOT NULL default '0' COMMENT 'Wenn etwas anderes als der Stundenpreis berechnet werden soll',
  `Rechnung_verschickt` tinyint(1) default NULL,
  `Rechnung_Datum` datetime default NULL,
  `rechnung_id` int(11) default NULL,
  `Bezahlt` tinyint(1) default NULL,
  `Bezahlt_Datum` timestamp NULL default NULL,
  `rechnungen_id` int(11) default NULL,
  PRIMARY KEY  (`einheiten_id`)
) ENGINE=InnoDB AUTO_INCREMENT=241 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

